<h1>Sessions#new</h1>

<%= form_tag sessions_path do %>
    <div>
        <%= label_tag :username %>
        <%= text_field_tag :username, params[:username] %>
    </div>
    <div>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>
    <div><%= submit_tag "Sign In" %></div>
<% end %>